<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-sm-4">
    <h2>Edit equipment profile</h2>
    <ol class="breadcrumb">
      <li>
        <%= link_to 'Equipment Profiles', equipment_profiles_path %>
      </li>
      <li class="active">
        <%= link_to @equipment_profile.id, @equipment_profile %>
      </li>
    </ol>
  </div>
</div>

<div class="wrapper wrapper-content">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox">
        <div class="ibox-title">
          <h5>Create equipment</h5>
        </div>
        <div class="ibox-content">
          <%= render 'form', equipment_profile: @equipment_profile %>
        </div>
      </div>
    </div>
  </div>
</div>
