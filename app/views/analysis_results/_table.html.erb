<div class="analysis-result analysis-result-table">
  <h2><%= result["key"] %></h2>
  <table class="table">
    <thead>
      <% result["table_columns"].each do |column| %>
        <th><%= column["key"] %></th>
      <% end %>
    </thead>
    <tbody>
      <% result["table_rows"].each do |row| %>
      <tr>
        <% row.each do |col| %>
          <td><%= col %></td>
        <% end %>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
