<div class="analysis-result analysis-result-table">
  <div class="table-responsive" style="display: inline-block">
    <table class="table">
      <thead>
        <% analysis_result.result_data["table_columns"].each do |column| %>
        <th><%= column["label"] || column["key"] || column %></th>
        <% end %>
      </thead>
      <tbody>
        <% analysis_result.result_data["table_rows"].each do |row| %>
        <tr>
          <% row.each do |col| %>
          <td><%= col %></td>
          <% end %>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
