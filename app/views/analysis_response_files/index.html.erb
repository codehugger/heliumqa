<p id="notice"><%= notice %></p>

<h1>Analysis Response Files</h1>

<table class="table">
  <thead>
    <tr>
      <th>Analysis response</th>
      <th>File data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analysis_response_files.each do |analysis_response_file| %>
      <tr>
        <td><%= analysis_response_file.analysis_response %></td>
        <td><%= analysis_response_file.file_data %></td>
        <td><%= link_to 'Show', analysis_response_file %></td>
        <td><%= link_to 'Edit', edit_analysis_response_file_path(analysis_response_file) %></td>
        <td><%= link_to 'Destroy', analysis_response_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis Response File', new_analysis_response_file_path %>
