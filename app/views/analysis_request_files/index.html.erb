<p id="notice"><%= notice %></p>

<h1>Analysis Request Files</h1>

<table class="table">
  <thead>
    <tr>
      <th>Analysis request</th>
      <th>Inspection file</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analysis_request_files.each do |analysis_request_file| %>
      <tr>
        <td><%= analysis_request_file.analysis_request %></td>
        <td><%= analysis_request_file.inspection_file %></td>
        <td><%= link_to 'Show', analysis_request_file %></td>
        <td><%= link_to 'Edit', edit_analysis_request_file_path(analysis_request_file) %></td>
        <td><%= link_to 'Destroy', analysis_request_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis Request File', new_analysis_request_file_path %>
