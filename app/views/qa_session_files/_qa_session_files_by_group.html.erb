<div class="panel-group" id="accordion">
  <% qa_session_files.order_alphanumeric(:filename).grouped_by_scan_series.each_with_index do |(series, qa_session_files), index| %>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion-sample" href="#collapse<%= index %>">
          <%= series.blank? ? 'Unmatched' : series.description %> (<%= qa_session_files.count %>)
        </a>
      </h4>
    </div>
    <div id="collapse<%= index %>" class="panel-collapse collapse in">
      <div class="panel-body">
        <%= render 'qa_session_files/qa_session_files', qa_session_files: qa_session_files %>
      </div>
    </div>
  </div>
  <% end %>
</div>
