<p id="notice"><%= notice %></p>

<h1>Analysis Requests</h1>

<table class="table">
  <thead>
    <tr>
      <th>Analysis</th>
      <th>Scan Protocol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analysis_requests.each do |analysis_request| %>
      <tr>
        <td><%= analysis_request.analysis %></td>
        <td><%= analysis_request.scan_protocol %></td>
        <td><%= link_to 'Show', analysis_request %></td>
        <td><%= link_to 'Edit', edit_analysis_request_path(analysis_request) %></td>
        <td><%= link_to 'Destroy', analysis_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis Request', new_analysis_request_path %>
