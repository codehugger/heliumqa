<p id="notice"><%= notice %></p>

<h1>Analysis Sessions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analysis_sessions.each do |analysis_session| %>
      <tr>
        <td><%= link_to 'Show', analysis_session %></td>
        <td><%= link_to 'Edit', edit_analysis_session_path(analysis_session) %></td>
        <td><%= link_to 'Destroy', analysis_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis Session', new_analysis_session_path %>
