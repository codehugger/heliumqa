<p id="notice"><%= notice %></p>

<h1>Inspection Files</h1>

<table class="table">
  <thead>
    <tr>
      <th>Inspection</th>
      <th>Equipment profile</th>
      <th>File data</th>
      <th>Profile header</th>
      <th>Profiled at</th>
      <th>Probed at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inspection_files.each do |inspection_file| %>
      <tr>
        <td><%= inspection_file.inspection %></td>
        <td><%= inspection_file.equipment_profile %></td>
        <td><%= inspection_file.file_data %></td>
        <td><%= inspection_file.profile_header %></td>
        <td><%= inspection_file.profiled_at %></td>
        <td><%= inspection_file.probed_at %></td>
        <td><%= link_to 'Show', inspection_file %></td>
        <td><%= link_to 'Edit', edit_inspection_file_path(inspection_file) %></td>
        <td><%= link_to 'Destroy', inspection_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inspection File', new_inspection_file_path %>
