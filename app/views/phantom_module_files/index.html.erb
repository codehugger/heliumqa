<p id="notice"><%= notice %></p>

<h1>Phantom Module Files</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phantom_module_files.each do |phantom_module_file| %>
      <tr>
        <td><%= link_to 'Show', phantom_module_file %></td>
        <td><%= link_to 'Edit', edit_phantom_module_file_path(phantom_module_file) %></td>
        <td><%= link_to 'Destroy', phantom_module_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phantom Module File', new_phantom_module_file_path %>
