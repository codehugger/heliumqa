<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-sm-12">
    <h2>Scan Series</h2>
    <ol class="breadcrumb">
      <li class="active">
        <strong>Scan Series</strong>
      </li>
    </ol>
  </div>
</div>

<div class="wrapper wrapper-content">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox">
        <div class="ibox-title">
          <h5>Scan Series</h5>
        </div>
        <div class="ibox-content">
          <% if @scan_series.count > 0 %>
            <%= render 'scan_series/scan_series', scan_series: @scan_series %>
          <% else %>
            <p>No scan series found.</p>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
